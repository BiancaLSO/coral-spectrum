                        "items": [
                            "justify#justifyleft",
                            "justify#justifycenter",
                            "justify#justifyright"
                        ]
                    },
                    "lists": {
                        "ref": "lists",
                        "icon": "text",
                        "items": [
                            "lists#insertunorderedlist",
                            "lists#insertorderedlist",
                            "lists#outdent",
                            "lists#indent"
                        ]
                    }
                }
