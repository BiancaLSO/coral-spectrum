    "uiSettings": {
        "cui": {
            "inline": {
                "toolbar": [
                    "#format",
                    "#justify",
                    "#lists"
                ],
                "popovers": {
                    "format": {
                        "ref": "format",
                        "icon": "text",
                        "items": [
                            "format#bold",
                            "format#italic",
                            "format#underline"
                        ]
                    },
                    "justify": {
                        "ref": "justify",
