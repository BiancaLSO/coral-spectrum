<?js
var self = this;
?>
<dl class="details">
    <?js
        var properties = obj.properties;
        if (properties && properties.length && properties.forEach) { 
    ?>
        
        <h4 class="line">Properties:</h4>
        
        <dl><?js= this.partial('properties.tmpl', properties) ?></dl>
        
    <?js } ?>
        
	<?js if (obj.version) {?>
	<dt class="tag-version">Version:</dt>
	<dd class="tag-version"><ul class="dummy"><li><?js= version ?></li></ul></dd>
	<?js } ?>
	
	<?js if (obj.since) {?>
	<dt class="tag-since">Since:</dt>
	<dd class="tag-since"><ul class="dummy"><li><?js= since ?></dd>
	<?js } ?>

	<?js if (obj.inherited && obj.inherits) { ?>
	<dt class="inherited-from">Inherited From:</dt>
	<dd class="inherited-from"><ul class="dummy"><li>
		<?js= this.linkto(obj.inherits, this.htmlsafe(obj.inherits)) ?>
	</li></dd>
	<?js } ?>
	
	<?js if (obj.deprecated) { ?>
        <dt class="important tag-deprecated">Deprecated:</dt><?js
            if (obj.deprecated === true) { ?><dd class="yes-def tag-deprecated"><ul class="dummy"><li>Yes</li></ul></dd><?js }
            else { ?><dd><ul class="dummy"><li><?js= obj.deprecated ?></li><ul></dd><?js }
        ?>
    <?js } ?>
	
	<?js if (obj.author && author.length) {?>
	<dt class="tag-author">Author:</dt>
	<dd class="tag-author">
        <ul><?js author.forEach(function(a) { ?>
            <li><?js= a ?></li>
        <?js }); ?></ul>
	</dd>
	<?js } ?>
	
	<?js if (obj.copyright) {?>
	<dt class="tag-copyright">Copyright:</dt>
	<dd class="tag-copyright"><ul class="dummy"><li><?js= copyright ?></li></ul></dd>
	<?js } ?>
	
	<?js if (obj.license) {?>
	<dt class="tag-license">License:</dt>
	<dd class="tag-license"><ul class="dummy"><li><?js= license ?></li></ul></dd>
	<?js } ?>
	
	<?js if (obj.defaultvalue) {?>
	<dt class="tag-default">Default Value:</dt>
	<dd class="tag-default"><ul class="dummy"><li><?js= obj.defaultvalue ?></li></ul></dd>
	<?js } ?>
	
	<?js if (obj.meta) {?>
	<dt class="tag-source">Source:</dt>
	<dd class="tag-source"><ul class="dummy"><li><?js= meta.filename ?>, line <?js= meta.lineno ?></li></ul></dd>
	<?js } ?>
    
    <?js if (obj.tutorials && tutorials.length) {?>
    <dt class="tag-tutorial">Tutorials:</dt>
    <dd class="tag-tutorial">
        <ul><?js tutorials.forEach(function(t) { ?>
            <li><?js= self.tutoriallink(t) ?></li>
        <?js }); ?></ul>
    </dd>
    <?js } ?>
	
	<?js if (obj.see && see.length) {?>
	<dt class="tag-see">See:</dt>
	<dd class="tag-see">
        <ul><?js see.forEach(function(s) { ?>
            <li><?js= self.linkto(s) ?></li>
        <?js }); ?></ul>
	</dd>
	<?js } ?>
	
	<?js if (obj.todo && todo.length) {?>
	<dt class="tag-todo">To Do:</dt>
	<dd class="tag-todo">
        <ul><?js todo.forEach(function(t) { ?>
            <li><?js= t ?></li>
        <?js }); ?></ul>
	</dd>
	<?js } ?>
</dl>
